<Window x:Class="WPF__App.Enter_name_window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF__App"
        mc:Ignorable="d"
        Title="Введите название эксперимента" Height="450" Width="800">
    <Grid>
        <TextBox Name="NameBox" HorizontalAlignment="Left" Margin="176,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="436" Height="40" Background="#FFFCF9C8" FontSize="24"/>
        <TextBlock HorizontalAlignment="Left" Margin="128,91,0,0" TextWrapping="Wrap" Text="Введите название эксперимента" VerticalAlignment="Top" Width="532" Height="88" FontSize="35" RenderTransformOrigin="0.495,0.399"/>
        <Button IsDefault="True" Content="Сохранить эксперимент" Click="Save_Click" HorizontalAlignment="Left" Margin="176,285,0,0" VerticalAlignment="Top" Width="436" Height="40" FontSize="24" Background="#FF12ED37"/>

    </Grid>
</Window>
